{
    "parser": "babel-eslint",
    "env":{
        "es6":true,
        "browser":true,
        "node":true
    },
    "rules": {
        "require-jsdoc": "off",
        "linebreak-style":0,
        "arrow-parens":"off",
        "comma-dangle":"off",
        "indent": [
            "error", 
            4, 
            {"SwitchCase": 1}
        ],
        "eol-last":"off",
        "operator-linebreak":"off"
    },
    "extends": [
        "eslint:recommended",
        "google"
        ]    
}